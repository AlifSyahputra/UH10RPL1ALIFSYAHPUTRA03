<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    card_view:cardCornerRadius="20dp"
    card_view:cardUseCompatPadding="true">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:orientation="vertical"
        android:padding="5dp">
        <ImageView
            android:layout_width="90dp"
            android:layout_height="100dp"
            android:translationX="150dp"
            android:translationY="20dp"
            android:src="@drawable/logobimbel"/>

        <TextView
            android:text="Pengajar"
            android:id="@+id/txt_namapengajar_logdata"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <TextView
            android:text="NamaPelajaran"
            android:id="@+id/txt_namapelajaran_logdata"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <TextView
            android:text="JumlahOrang"
            android:id="@+id/txt_jumlahorang_logdata"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <TextView
            android:text="tanggal"
            android:id="@+id/txttanggal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>
</androidx.cardview.widget.CardView>
